        };

        PROG_arrow: PROG_arrow { 
            label = "PROG_arrow";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&macro_tap &kp MINUS &kp GREATER_THAN>;
        };
    };


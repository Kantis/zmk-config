        };

        SWE_oumlaut: SWE_oumlaut {
            label = "SWE_oumlaut";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp LALT>
                , <&macro_tap &kp O>
                , <&macro_release &kp LALT>
